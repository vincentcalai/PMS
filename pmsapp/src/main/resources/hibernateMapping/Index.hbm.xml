<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="com.pms.pmsapp.common.data.Index" table="PMS_INDEX">
    
    <id name="id" type="java.lang.Long" column="ID">
    </id>

    <property
        name="idxSym"
        type="java.lang.String"
        column="INDEX_SYM"
        length="10"
    />
    <property
        name="idxName"
        type="java.lang.String"
        column="INDEX_NAME"
        length="50"
    />
    <property
        name="last"
        type="java.math.BigDecimal"
        column="LAST"
    />
    <property
        name="change"
        type="java.math.BigDecimal"
        column="CHANGE"
    />
    <property
        name="changePct"
        type="java.math.BigDecimal"
        column="CHANGE_PCT"
    />
    <property
        name="lastUpdatedDt"
        type="java.util.Date"
        column="LAST_UPDATED_DT"
    />
</class>
</hibernate-mapping>
