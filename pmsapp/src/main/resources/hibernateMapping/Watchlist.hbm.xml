<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="com.pms.pmsapp.watchlist.data.Watchlist" table="PMS_WATCHLIST">
    <id name="id" type="java.lang.Long" column="ID" length="10">
    </id>
    <property
        name="name"
        type="java.lang.String"
        column="NAME"
        length="50"
    />
    <property
        name="remarks"
        type="java.lang.String"
        column="REMARKS"
        length="100"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="20"
    />
    <property
        name="createdDt"
        type="java.util.Date"
        column="CREATED_DT"
    />
    <property
        name="lastMdfyBy"
        type="java.lang.String"
        column="LAST_MDFY_BY"
        length="20"
    />
    <property
        name="lastMdfyDt"
        type="java.sql.Timestamp"
        column="LAST_MDFY_DT"
    />
</class>
</hibernate-mapping>
