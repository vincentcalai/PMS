<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class name="com.pms.pmsapp.portfolio.data.Portfolio" table="PMS_PORT">
    
    <id name="id" type="java.lang.Long" column="ID">
      <generator class="sequence">
          <param name="sequence_name">SQ_PMS_PORT</param>
      </generator>
    </id>

    <property
        name="portfolioName"
        type="java.lang.String"
        column="PORT_NAME"
        length="25"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="CREATED_BY"
        length="20"
    />
    <property
        name="createdDate"
        type="java.util.Date"
        column="CREATED_DT"
    />
    <property
        name="lastMdfyBy"
        type="java.lang.String"
        column="LAST_MDFY_BY"
        length="20"
    />
    <property
        name="lastMdfyDt"
        type="java.util.Date"
        column="LAST_MDFY_DT"
        length="11"
    />
    <property
        name="remarks"
        type="java.lang.String"
        column="REMARKS"
        length="100"
    >
    </property>
</class>
</hibernate-mapping>
