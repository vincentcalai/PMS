<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class name="com.pms.pmsapp.portfolio.data.PortfolioHold" table="PMS_PORT_HOLD">
    
    <composite-id>
		<key-property name="portId" length="10" type="java.lang.Long" column="PORT_ID" />
		<key-property name="stockSymbol" length="6" type="java.lang.String" column="STOCK_SYM" />
	</composite-id>

    <property
        name="stockName"
        type="java.lang.String"
        column="STOCK_NAM"
        length="100"
    />
    <property
        name="stockExchg"
        type="java.lang.String"
        column="STOCK_EXCHG"
        length="6"
    />
    <property
        name="totalShare"
        type="int"
        column="TOTAL_SHARE"
    />
    <property
        name="avgPrice"
        type="java.math.BigDecimal"
        column="AVG_PRICE"
    >
    </property>
    <property
        name="totalAmt"
        type="java.math.BigDecimal"
        column="TOTAL_AMT"
    >
    </property>
    <property
        name="lastTransPrice"
        type="java.math.BigDecimal"
        column="LAST_TRANS_PRICE"
    >
    </property>
    <property
        name="mktValue"
        type="java.math.BigDecimal"
        column="MKT_VALUE"
    >
    </property>
    <property
        name="profitLoss"
        type="java.math.BigDecimal"
        column="PROFIT_LOSS"
    >
    </property>
    <property
        name="profitLossPct"
        type="java.math.BigDecimal"
        column="PROFIT_LOSS_PCT"
    >
    </property>
    <property
        name="lastTransBy"
        type="java.lang.String"
        column="LAST_TRANS_BY"
    >
    </property>
    <property
        name="lastTransDt"
        type="java.util.Date"
        column="LAST_TRANS_DT"
    >
    </property>
</class>
</hibernate-mapping>
