<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="com.pms.pmsapp.watchlist.data.WatchlistEntry" table="PMS_WATCHLIST_ENTRY">
    
    <id name="id" type="java.lang.Long" column="ID" length="10">
    </id>
	<property
        name="watchId"
        type="java.lang.Long"
        column="WATCH_ID"
        length="10"
    />
    <property
        name="stockName"
        type="java.lang.String"
        column="STOCK_NAM"
        length="100"
    />
    <property
        name="stockSym"
        type="java.lang.String"
        column="STOCK_SYM"
        length="10"
    />
    <property
        name="stockExchg"
        type="java.lang.String"
        column="STOCK_EXCHG"
        length="6"
    />
    <property
        name="lastPrice"
        type="java.math.BigDecimal"
        column="LAST_PRICE"
    />
    <property
        name="change"
        type="java.math.BigDecimal"
        column="CHANGE"
    >
    </property>
    <property
        name="changePct"
        type="java.math.BigDecimal"
        column="CHANGE_PCT"
    >
    </property>
    <property
        name="targetPrice"
        type="java.math.BigDecimal"
        column="TARGET_PRICE"
    >
    </property>
    <property
        name="premiumDisc"
        type="java.math.BigDecimal"
        column="PREMIUM_DISC"
    >
    </property>
    <property
        name="remarks"
        type="java.lang.String"
        column="REMARKS"
    >
    </property>
    <property
        name="lastMdfyBy"
        type="java.lang.String"
        column="LAST_MDFY_BY"
    >
    </property>
    <property
        name="lastMdfyDt"
        type="java.sql.Timestamp"
        column="LAST_MDFY_DT"
    >
    </property>
</class>
</hibernate-mapping>
